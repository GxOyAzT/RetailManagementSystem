@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model List<Models.ShopModel>

<h1>Home/LoginPointOfSalesList</h1>

<table>
    <thead>
        <tr>
            <th>City</th>
            <th>Street</th>
            <th>LogIn</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@(item.City)</td>
                <td>@(item.Street)</td>
                <td>
                    <a asp-controller="Home" asp-action="Login" asp-route-shopId="@(item.Id.ToString())">Log In</a>
                </td>
            </tr>
        }
    </tbody>
</table>